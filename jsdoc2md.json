{
  "plugins": ["plugins/markdown", "plugins/summarize"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "recurseDepth": 0,
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./src/helpers"]
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": false
  }
}
