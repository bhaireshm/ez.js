{
  "name": "@bhairesh/ez.js",
  "version": "0.0.11",
  "description": "Helper and utility functions.",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "dev": "nodemon ./index.js",
    "test": "node ./src/tests/",
    "dist": "rollup -c",
    "prepare": "npx husky install",
    "prepublishOnly": "npm run dist && npm test && npm run docs:md",
    "eslint": "eslint *.js src/**/*.js",
    "eslint:fix": "eslint --fix *.js src/**/*.js",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier  --write .",
    "lint-staged": "lint-staged",
    "docs:md": "jsdoc2md --template ./docs/template.hbs --no-cache -c jsdoc2md.json --files ./src/helpers/* > README.md"
  },
  "author": "Bhairesh M <bhairesh@arithaconsulting.com>",
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "javascript",
    "utils"
  ],
  "devDependencies": {
    "@bhairesh/ez.js": "^0.0.10",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.46.0",
    "eslint-plugin-import": "^2.28.0",
    "jsdoc-to-markdown": "^8.0.0",
    "lint-staged": "^13.2.3",
    "nodemon": "^2.0.21",
    "prettier": "^3.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
