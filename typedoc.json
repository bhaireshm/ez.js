{
  "out": "docs",
  "mode": "modules",
  "includeDeclarations": true,
  "excludeExternals": true,
  "excludeNotExported": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "target": "ES6",
  "module": "CommonJS",
  "plugins": ["typedoc-plugin-markdown", "typedoc-plugin-summary"],
  "theme": "default",
  "ignoreCompilerErrors": true,
  "tsconfig": "./tsconfig.json" // Replace with the actual path to your tsconfig.json file
}
